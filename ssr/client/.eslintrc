{
  "extends": "airbnb",  
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": [0],
    "comma-dangle": [0],
    "jsx-quotes": [0],
    "lines-between-class-members": [0],
    "react/jsx-filename-extension": [0]
  }

}
